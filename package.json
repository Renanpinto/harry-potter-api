    "emits": "^3.0.0",
    "event-emitter": "^0.3.5",
    "express": "^4.16.3",
    "faker": "^4.1.0",
    "joi": "^13.2.0",
    "knex": "^0.14.3",
    "pg": "^7.4.3",
    "typeorm": "^0.2.7",
    "uuid": "^3.2.1",
    "babel-cli": "^6.26.0",
    "babel-eslint": "^8.2.6",
    "babel-preset-node8": "^1.2.0",
    "babel-register": "^6.26.0",
    "eslint": "^5.14.0",
    "eslint-config-airbnb-base": "^13.1.0",
